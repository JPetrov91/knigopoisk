<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Register</servlet-name>
  	<display-name>Register</display-name>
  	<description></description>
  	<servlet-class>Servlets.Register</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginValidation</servlet-name>
  	<display-name>LoginValidation</display-name>
  	<description></description>
  	<servlet-class>Servlets.LoginValidation</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Register</servlet-name>
  	<url-pattern>/Register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginValidation</servlet-name>
  	<url-pattern>/LoginValidation</url-pattern>
  </servlet-mapping>
</web-app>
